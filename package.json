{
  "name": "fiscalapi",
  "version": "4.0.124",
  "description": "Genera facturas CFDI válidas ante el SAT consumiento el api de https://www.fiscalapi.com",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "jest",
    "lint": "eslint 'src/**/*.ts'",
    "prepare": "npm run build"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "keywords": [
    "factura",
    "cfdi",
    "facturacion",
    "mexico",
    "sat"
  ],
  "author": "FISCAL API S DE R.L DE C.V",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FiscalAPI/fiscalapi-node"
  },
  "homepage": "https://www.fiscalapi.com",
  "engines": {
    "node": ">=12.0.0"
  },
  "copyright": "FISCAL API S DE R.L DE C.V © 2019",
  "dependencies": {
    "axios": "^1.8.4",
    "luxon": "^3.6.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.14",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}